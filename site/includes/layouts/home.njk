---
layout: base
---
<main id="main" class="main box-flex pb-1100" tabindex="-1">
    <div id="wrapper" class="wrapper box-flex pb-900">
        <header class="page-header">

            <h1 class="page-title">{{ title }}</h1>

            {% if sub_heading %}

                <p class="sub-heading">{{ sub_heading }}</p>

            {% endif %}

        </header>
    </div>
    <div class="inner pr-600 pl-600 content flow">

        {{ content | safe }}

    </div>

</main>
